func add(a, b) => a + b
func min(a, b) => a - b
func mul(a, b) => a * b
func div(a, b) => a / b
func mod(a, b) => a % b
func pow(a, b) => a ^ b

func max(list) {
    dim last_item = 0
    dim current_item = 0
    dim tmp = []
    dim length = len(list)

    for i=0 to length then
        dim current_item = list : i
        if current_item > last_item then
            append(tmp, current_item)
            dim last_item = current_item
        end
    end

    ret tmp : (len(tmp) - 1)
}

func min(list) {
    dim last_item = MATH_POS_INF
    dim current_item = 0
    dim tmp = []
    dim length = len(list)

    for i=0 to length then
        dim current_item = list : i
        if current_item < last_item then
            append(tmp, current_item)
            dim last_item = current_item
        end
    end

    ret tmp : (len(tmp) - 1)
}
